{
  "name": "coderatio/laranotify",
  "description": "A feature-rich laravel package that turns bootstrap notify jQuery plugin to dynamic notifications, alerts and blockables.",
  "license": "MIT",
  "keywords": ["coderatio", "code9ja", "laravel", "lara", "larnotify", "laravel notification", "laravel framework", "laranotify", "notify", "notifier", "bootstrap notify"],
  "type": "laravel-package",

  "authors": [
    {
      "name": "Josiah Ovye Yahaya",
      "email": "josiahoyahaya@gmail.com"
    }
  ],

  "minimum-stability": "dev",

  "require": {
    "php": ">=5.6.0",
    "illuminate/support": ">=4.0"
  },

  "autoload": {
    "psr-4": {
      "Coderatio\\Laranotify\\": "src/"
    }
  },

  "extra": {
    "laravel": {
      "providers": [
        "Coderatio\\Laranotify\\LaranotifyServiceProvider"
      ],
      "aliases": {
        "Notify": "Coderatio\\Laranotify\\Facades\\Notify",
        "Laranotify": "Coderatio\\Laranotify\\Facades\\Laranotify"
      }
    }
  }
}
